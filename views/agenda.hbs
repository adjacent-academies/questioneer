<div class="ui container">
  <h1>{{title}}</h1>
  <div class="ui hidden divider"></div>
  <div class="ui agenda cards">
    {{#each top}}
    <div class="ui fluid raised card" data-card-id="{{id}}">
      <div class="content">
        <i class="ui right floated circle link big gray icon check outline"></i>
        <div class="ui right floated red label">{{points}} points</div>
        <div class="header">{{text}}</div>
        <div class="meta">submitted on {{formatDate timestamp}}</div>
      </div>
    </div>
    {{/each}}
  </div>
</div>